{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/order/list/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/personal/core/index",
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "rgba(153, 153, 153, 1)",
		"selectedColor": "rgba(252, 114, 0, 1)",
		"borderStyle": "white",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/home/index",
				"iconPath": "static/image/home.png",
				"selectedIconPath": "static/image/home_HL.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/order/list/index",
				"iconPath": "static/image/order.png",
				"selectedIconPath": "static/image/order_HL.png",
				"text": "订单"
			},
			{
				"pagePath": "pages/personal/core/index",
				"iconPath": "static/image/personal.png",
				"selectedIconPath": "static/image/personal_HL.png",
				"text": "我的"
			}
		]
	}
}